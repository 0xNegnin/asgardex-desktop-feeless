{
  "name": "asgardex",
  "productName": "ASGARDEX",
  "version": "0.6.0",
  "description": "WALLET AND EXCHANGE CLIENT FOR THORCHAIN",
  "main": "build/index.cjs",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "dev:clean": "npm run clean:build && npm run dev",
    "force-rebuild-native-deps": "rm -rf node_modules/prebuild-install",
    "build": "npm run clean:build && node scripts/build.mjs",
    "package": "npm run build && npm run package:electron",
    "package:electron": "electron-builder build -c.extraMetadata.main=build/main/index.cjs --config .electronbuildrc",
    "package:ci": "npm run postinstall && npm run build && npm run package:electron --publish always",
    "postinstall": "git config blame.ignoreRevsFile .git-blame-ignore-revs && node scripts/fix.sr25519.js && npm run force-rebuild-native-deps && electron-builder install-app-deps",
    "lint": "eslint \"{src,e2e,}/**/*.{ts,tsx}\" --fix",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "//test:e2e": "npm run build && npm run test:cafe",
    "test:cafe": "cross-env NODE_ENV=production testcafe electron:./ ./e2e/**/*.e2e.ts",
    "generate:erc20whitelist": "./node_modules/.bin/ts-node --project tsconfig.scripts.json ./scripts/generateERC20Whitelist.ts",
    "generate:types": "npm run clean:types:midgard && npm run generate:types:midgard",
    "generate:types:midgard": "TS_POST_PROCESS_FILE=./node_modules/.bin/prettier openapi-generator-cli generate -i https://testnet.midgard.thorchain.info/v2/swagger.json -g typescript-rxjs -o ./src/renderer/types/generated/midgard --reserved-words-mappings in=in --enable-post-process-file",
    "clean:types:midgard": "rm -rf ./src/renderer/types/generated/midgard",
    "clean:build": "rm -rf ./node_modules/.vite && rm -rf ./build/**/*",
    "//TODO(@veado) 2": "Make storybook run",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thorchain/asgardex-electron"
  },
  "author": {
    "name": "ASGARDEX Maintainers",
